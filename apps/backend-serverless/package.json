{
  "name": "backend-serverless",
  "version": "0.0.1",
  "private": true,
  "description": "",
  "main": "handler.js",
  "workspaces": {
    "nohoist": [
      "**"
    ]
  },
  "type": "module",
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "scripts": {
    "predev": "yarn helius & npx prisma generate",
    "dev": "NODE_EXTRA_CA_CERTS=dev-certs/cert.pem serverless offline start --stage dev --config serverless.purple.yml & NODE_EXTRA_CA_CERTS=dev-certs/cert.pem serverless offline start --stage dev --config serverless.green.yml",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "test": "jest",
    "test:tx": "jest integration",
    "helius": "node scripts/helius.cjs",
    "deploy:prod:green": "serverless deploy --stage prod --config serverless.green.yml",
    "deploy:prod:purple": "serverless deploy --stage prod --config serverless.purple.yml",
    "prisma:push:dev": "dotenv -e .env.dev -- npx prisma db push",
    "prisma:push:stage": "dotenv -e .env.stage -- npx prisma db push",
    "prisma:push:prod": "dotenv -e .env.prod -- npx prisma db push",
    "prisma:studio:dev": "dotenv -e .env.dev -- npx prisma studio",
    "prisma:studio:prod": "dotenv -e .env.prod -- npx prisma studio"
  },
  "keywords": [],
  "author": "@tj_littlejohn",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "4.13.0",
    "@sentry/node": "^7.51.2",
    "@sentry/serverless": "^7.51.2",
    "@sentry/tracing": "^7.51.2",
    "@solana/spl-token": "^0.3.7",
    "@solana/web3.js": "^1.77.3",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/node": "18.15.3",
    "aws-lambda": "^1.0.7",
    "aws-sdk": "^2.1382.0",
    "axios": "^1.3.4",
    "bs58": "5.0.0",
    "crypto-js": "^4.1.1",
    "dotenv": "^16.0.3",
    "helius-sdk": "^1.0.7",
    "jsonwebtoken": "^9.0.0",
    "lodash": "^4.17.21",
    "prisma": "4.13.0",
    "yup": "1.2.0"
  },
  "devDependencies": {
    "@ngrok/ngrok": "^0.5.2",
    "@types/aws-lambda": "^8.10.101",
    "@types/crypto-js": "4.1.1",
    "@types/jest": "^29.5.1",
    "@types/winston": "^2.4.4",
    "@typescript-eslint/eslint-plugin": "^5.59.1",
    "@typescript-eslint/parser": "^5.59.1",
    "axios-mock-adapter": "^1.21.4",
    "dotenv-cli": "^7.2.1",
    "esbuild": "^0.17.18",
    "eslint": "^8.39.0",
    "eslint-plugin-require-extensions": "^0.1.3",
    "jest": "^29.5.0",
    "jest-mock-extended": "2.0.4",
    "rimraf": "^5.0.1",
    "serverless": "^3.21.0",
    "serverless-esbuild": "^1.43.1",
    "serverless-iam-roles-per-function": "^3.2.0",
    "serverless-offline": "^12.0.4",
    "serverless-plugin-common-excludes": "^4.0.0",
    "serverless-plugin-include-dependencies": "^5.1.0",
    "serverless-plugin-typescript": "2.1.4",
    "serverless-prune-plugin": "^2.0.2",
    "serverless-step-functions": "^3.13.1",
    "ts-jest": "^29.1.0",
    "tsx": "^3.12.7",
    "typescript": "^4.7.4",
    "winston": "^3.9.0"
  }
}