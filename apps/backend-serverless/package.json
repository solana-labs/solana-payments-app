{
    "name": "backend-serverless",
    "version": "1.0.0",
    "private": true,
    "description": "",
    "main": "handler.js",
    "workspaces": {
        "nohoist": [
            "**"
        ]
    },
    "type": "module",
    "prisma": {
        "seed": "tsx prisma/seed.ts"
    },
    "scripts": {
        "start-ngrok": "ngrok http 4000 > /dev/null & echo $! > ngrok.pid",
        "setup:helius": "node scripts/helius.cjs",
        "predev": "yarn start-ngrok && yarn setup:helius",
        "dev": "serverless offline start --stage dev --config serverless.purple.yml & serverless offline start --stage dev --config serverless.green.yml",
        "build": "tsc",
        "test": "jest",
        "test:tx": "jest integration",
        "deploy:production:green": "serverless deploy --stage production --config serverless.green.yml",
        "deploy:staging:green": "serverless deploy --stage staging --config serverless.green.yml",
        "deploy:staging:purple": "serverless deploy --stage staging --config serverless.purple.yml",
        "deploy:production:purple": "serverless deploy --stage production --config serverless.purple.yml",
        "prisma:push:dev": "dotenv -e .env.dev -- npx prisma db push",
        "prisma:push:staging": "dotenv -e .env.staging -- npx prisma db push",
        "prisma:push:production": "dotenv -e .env.production -- npx prisma db push",
        "prisma:studio:dev": "dotenv -e .env.dev -- npx prisma studio",
        "prisma:studio:staging": "dotenv -e .env.staging -- npx prisma studio",
        "prisma:studio:production": "dotenv -e .env.production -- npx prisma studio",
        "prisma:generate": "npx prisma generate",
        "prisma:seed:dev": "dotenv -e .env.dev -- npx prisma db seed",
        "lint": "eslint .",
        "format": "prettier --write \"**/*.{ts,tsx,md}\""
    },
    "keywords": [],
    "author": "@solana_labs",
    "license": "ISC",
    "dependencies": {
        "@metaplex-foundation/js": "^0.19.4",
        "@prisma/client": "^4.13.0",
        "@project-serum/anchor": "^0.26.0",
        "@sentry/node": "^7.51.2",
        "@sentry/serverless": "^7.60.0",
        "@sentry/tracing": "^7.51.2",
        "@solana/spl-token": "^0.3.7",
        "@solana/web3.js": "^1.77.3",
        "@types/jsonwebtoken": "^9.0.2",
        "aws-lambda": "^1.0.7",
        "aws-sdk": "^2.1382.0",
        "axios": "^1.3.4",
        "bs58": "5.0.0",
        "dotenv": "^16.0.3",
        "helius-sdk": "^1.0.7",
        "jsonwebtoken": "^9.0.0",
        "lodash": "^4.17.21",
        "node-fetch": "^3.3.1",
        "prisma": "^4.13.0",
        "yup": "1.2.0"
    },
    "devDependencies": {
        "@ngrok/ngrok": "^0.5.2",
        "@types/aws-lambda": "^8.10.101",
        "@types/jest": "^29.5.3",
        "@types/node": "^18.16.1",
        "@types/winston": "^2.4.4",
        "axios-mock-adapter": "^1.21.4",
        "concurrently": "^8.2.0",
        "dotenv-cli": "^7.2.1",
        "eslint-config-custom": "*",
        "jest": "^27.0.0",
        "jest-mock-extended": "2.0.4",
        "rimraf": "^5.0.1",
        "serverless": "^3.33.0",
        "serverless-esbuild": "^1.43.1",
        "serverless-iam-roles-per-function": "^3.2.0",
        "serverless-offline": "^12.0.4",
        "serverless-offline-sqs": "^7.3.2",
        "serverless-plugin-common-excludes": "^4.0.0",
        "serverless-plugin-include-dependencies": "^5.1.0",
        "serverless-plugin-optimize": "4.2.1-rc.1",
        "serverless-plugin-typescript": "^2.1.5",
        "serverless-prune-plugin": "^2.0.2",
        "serverless-prune-versions": "^1.0.4",
        "serverless-s3-local": "^0.7.2",
        "serverless-step-functions": "^3.13.1",
        "ts-jest": "^27.0.0",
        "tsx": "^3.12.7",
        "typescript": "^5.0.3",
        "winston": "^3.9.0"
    },
    "browser": {
        "crypto": false
    },
    "packageManager": "yarn@3.6.1"
}
