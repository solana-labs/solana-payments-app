{
  "$schema": "https://turbo.build/schema.json",
  "globalDependencies": ["**/.env.*local"],
  "pipeline": {
    "build:payment-ui": {
      "outputs": ["./apps/payment-ui/.next/**"]
    },
    "build:merchant-ui": {
      "outputs": ["./apps/merchant-ui/.next/**"]
    },
    "build": {
      "dependsOn": ["build:payment-ui", "build:merchant-ui"],
      "outputs": []
    },
    "lint": {},
    "dev": {
      "cache": false
    }
  }
}
