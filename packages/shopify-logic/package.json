{
    "name": "shopify-logic",
    "version": "0.0.1",
    "description": "Package containing all of the logic specific to interacting with Shopify.",
    "main": "./dist/index.js",
    "module": "./dist/index.mjs",
    "types": "./dist/index.d.ts",
    "files": [
        "dist/**"
    ],
    "keywords": [
        "api"
    ],
    "auther": "@tj_littlejohn",
    "scripts": {
        "build": "tsup src/index.ts --format cjs,esm --dts",
        "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
        "dev": "tsup src/index.ts --format cjs,esm --watch --dts",
        "lint": "TIMING=1 eslint \"src/**/*.ts\"",
        "test": "jest"
    },
    "devDependencies": {
        "axios": "^0.24.0",
        "eslint": "^7.32.0",
        "jest": "^29.5.0",
        "query-string": "^7.1.2",
        "ts-jest": "^29.0.5",
        "tsup": "^6.2.3",
        "typescript": "^4.8.3",
        "crypto-js": "^4.1.1"
    },
    "dependencies": {
        "@types/jest": "^29.5.1"
    }
}