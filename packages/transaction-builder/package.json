{
    "name": "transaction-builder",
    "version": "0.0.1",
    "description": "Higher level composable transaction building package on top of web3.js and other transactions that can be fetched via APIs.",
    "main": "./dist/index.js",
    "module": "./dist/index.mjs",
    "types": "./dist/index.d.ts",
    "files": [
        "dist/**"
    ],
    "keywords": [
        "api"
    ],
    "auther": "@tj_littlejohn",
    "scripts": {
        "build": "tsup src/index.ts --format cjs,esm --dts",
        "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
        "dev": "tsup src/index.ts --format cjs,esm --watch --dts",
        "lint": "TIMING=1 eslint \"src/**/*.ts\"",
        "test": "jest"
    },
    "devDependencies": {
        "axios": "^0.24.0",
        "eslint": "^7.32.0",
        "jest": "^29.5.0",
        "ts-jest": "^29.0.5",
        "tsup": "^6.2.3",
        "typescript": "^4.8.3"
    },
    "dependencies": {
        "@project-serum/anchor": "^0.26.0",
        "zod": "^3.21.4"
    }
}
